{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
		"paths": {
			"@/components/*": ["src/components/*.astro"],
			"@/layouts/*": ["src/layouts/*.astro"],
			"@/utils": ["src/utils/index.ts"],
			"@/stores/*": ["src/stores/*"],
			"@/data/*": ["src/data/*"],
			"@/site-config": ["src/site.config.ts"]
		}
  }
}